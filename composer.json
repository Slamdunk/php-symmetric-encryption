{
    "name": "slam/php-symmetric-encryption",
    "description": "PHP Symmetric Encryption",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/slamdunk/php-symmetric-encryption",
    "require": {
        "php": "~8.3.0 || ~8.4.0",
        "ext-sodium": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.80.0",
        "infection/infection": "^0.30.1",
        "phpstan/phpstan": "^2",
        "phpstan/phpstan-phpunit": "^2",
        "phpunit/phpunit": "^12.2.6"
    },
    "autoload": {
        "psr-4": {
            "SlamSymmetricEncryption\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlamSymmetricEncryption\\Test\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
