{
    "name": "slam/php-symmetric-encryption",
    "description": "PHP Symmetric Encryption",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/slamdunk/php-symmetric-encryption",
    "require": {
        "php": "~8.1.0 || ~8.2.0",
        "ext-sodium": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^v3.38.0",
        "infection/infection": "^0.27.8",
        "phpstan/phpstan": "^1.10.41",
        "phpstan/phpstan-phpunit": "^1.3.15",
        "phpunit/phpunit": "^10.4.2"
    },
    "autoload": {
        "psr-4": {
            "SlamSymmetricEncryption\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlamSymmetricEncryption\\Test\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
